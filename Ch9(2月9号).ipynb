   "source": [
    "# 演示内置产生迭代函数iter()\n",
    "# p111 【实例9-2】\n",
    "class Counter:\n",
    "    def __init__(self, x=0):\n",
    "        self.x = x\n",
    "        \n",
    "counter = Counter()\n",
    "\n",
    "def used_iter():\n",
    "    counter.x += 2\n",
    "    return counter.x\n",
    "\n",
    "for i in iter(used_iter, 8):\n",
    "    print('本次迭代函数值：', i)"
   ]
